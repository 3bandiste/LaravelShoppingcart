{
    "name": "eric-tromas/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Tromas",
            "email": "eric.tromas@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.4.* || 5.5.*|| 5.6.* || 5.7.* || 5.8.*",
        "illuminate/session": "5.4.* || 5.5.*|| 5.6.* || 5.7.* || 5.8.*",
        "illuminate/events": "5.4.* || 5.5.*|| 5.6.* || 5.7.* || 5.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "EricTromas\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EricTromas\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "EricTromas\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "EricTromas\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
